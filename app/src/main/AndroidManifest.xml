<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.kampusmerdeka.finalproject4">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FinalProject4"
        tools:targetApi="31">
        <activity
            android:name=".Started.FlashScreenActivity"
            android:exported="true"
            android:label="Flash Screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Started.LoginActivity"
            android:exported="false"
            android:label="Login" />
        <activity
            android:name=".NavBarActivity"
            android:exported="false"
            android:label="@string/title_activity_nav_bar" />
        <activity
            android:name=".Started.GetStartedActivity"
            android:exported="false" />
        <activity
            android:name=".Started.RegistPhoneActivity"
            android:exported="false"
            android:label="Register Phone" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
    </application>

</manifest>